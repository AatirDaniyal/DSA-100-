
#include<iostream>
using namespace std;

int main()
{
    string s = "abbabbba";

    for(int i=0; i<s.length(); i++){

        if(s[i] == 'a'){
            s[i] = 'b'; 
        }  
        else if(s[i] == 'b'){
            s[i] ='a';
        }
        else 
        { }
    }
    cout<<"output is :"<<s;

}
